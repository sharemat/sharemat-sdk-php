{
  "name": "sharemat/php-sdk",
  "description": "PHP SDK for Sharemat REST API",
  "version": "0.0.2",
  "type": "package",
  "license": "mit",
  "authors": [
    {
      "name": "Aurélien François",
      "email": "aurelien.francois@lephare.com",
      "homepage": "https://www.lephare.com/"
    }
  ],
  "require": {
    "php": ">=7.3",
    "ext-json": "*",
    "guzzlehttp/guzzle": "^7.0",
    "nyholm/psr7": "^1.3",
    "symfony/http-client": "^5.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^9",
    "symfony/dotenv": "^5.1"
  },
  "config": {
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true,
    "platform": {
      "php": "7.3.12",
      "ext-mbstring": "1"
    }
  },
  "scripts": {
    "phpunit": [
      "php -d memory_limit=-1 vendor/bin/phpunit --order-by=defects"
    ]
  },
  "autoload": {
    "psr-4": {
      "Sharemat\\Sdk\\": "src/"
    }
  }
}
